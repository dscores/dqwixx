  "version": "1.0.0",
  "homepage": "http://github.com/dscores/dqwixx",
  "private": true,
  "readmeFilename": "README.md",
  "scripts": {
    "postinstall": "run-p postinstall:*",
    "postinstall:bower": "bower i",
    "postinstall:dlogos": "dlogos",
    "postinstall:node-sass": "node-sass sass/app.scss www/app.css",
    "postinstall:riot": "riot tags/app.tag www/app.js",
